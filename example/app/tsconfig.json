{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": false,
    "esModuleInterop": false,
    "module": "ESNext",
    "composite": true,
    "outDir": "out/ts",
    "rootDir": "/",
    "sourceMap": false,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "types": [
      "vite/client"
    ]
  },
  "include": [
    "src",
    "src/appdata/whitelist.json",
    "./.graphclient/**/*"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    },
    {
      "path": "../contract"
    },
    {
      "path": "../../packages/composooor"
    }
  ]
}
